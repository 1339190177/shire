{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "patterns": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "pattern": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string"
              },
              "regex": {
                "type": "string"
              },
              "confidence": {
                "type": "string",
                "enum": ["high", "medium", "low"]
              }
            },
            "required": ["name", "regex", "confidence"]
          }
        },
        "required": ["pattern"]
      }
    }
  },
  "required": ["patterns"]
}
